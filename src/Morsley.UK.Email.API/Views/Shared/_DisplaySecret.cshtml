@model Morsley.UK.Email.API.Models.DisplaySecret

@{
}

@if (!string.IsNullOrEmpty(ViewBag.TestSecret))
{
    <div class="alert alert-success">
        <strong>@(Model.Comment):</strong> @Model.Value
    </div>
}
else
{
    <div class="alert alert-warning">
        Secret not found or empty
    </div>
}